{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "monitoring_result",
  "title": "monitoring, schema for result",
  "type": "object",
  "properties": {
    "cpu": {
      "type": "string",
      "enum": ["ok", "low", "high"]
    },
    "memory": {
      "type": "string",
      "enum": ["ok", "low", "high"]
     },
    "disk": {
      "type": "string",
      "enum": ["ok", "low", "high"]
     }
  },
  "required": [
    "cpu",
    "memory"
  ]
}